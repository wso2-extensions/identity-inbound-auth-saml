<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="identity-inbound-query-saml-Test-Suite">

    <test name="query-saml-test-all">
        <classes>
            <class name="org.wso2.carbon.identity.query.saml.dto.InvalidItemDTOTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.AbstractSAMLQueryValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLValidatorFactoryTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLIDRequestValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLSubjectQueryValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLAuthQueryValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLAttributeQueryValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.validation.SAMLAuthzDecisionValidatorTest"/>
            <class name="org.wso2.carbon.identity.query.saml.X509CredentialImplTest"/>
            <class name="org.wso2.carbon.identity.query.saml.QueryResponseBuilderTest"/>
            <class name="org.wso2.carbon.identity.query.saml.processor.SAMLProcessorFactoryTest"/>
            <class name="org.wso2.carbon.identity.query.saml.processor.SAMLSubjectQueryProcessorTest"/>
        </classes>
    </test>
</suite>
